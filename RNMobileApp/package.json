{
  "name": "@flawlesspt/yellow-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "yarn install_dependencies && yarn pod-install",
    "start": "react-native start",
    "start-clear": "react-native start --reset-cache",
    "clean-project": "cd ios && rm -rf build && cd ../android && ./gradlew clean && cd ..",
    "install_dependencies": "rm -rf node_modules && yarn install",
    "pod-install": "cd ios && rm -rf Pods && pod install && cd ..",
    "clean-pod-install": "cd ios && rm -rf Pods && pod deintegrate && pod install && cd ..",
    "list-ios-devices": "xcrun simctl list devices",
    "android-dev": "ENVFILE=.env.dev react-native run-android",
    "android-dev-release": "ENVFILE=.env.dev react-native run-android --mode=Release --appId=pt.rainbowapps.gardao.app.release",
    "android-staging": "ENVFILE=.env.stg react-native run-android",
    "android-staging-release": "ENVFILE=.env.stg react-native run-android --mode=Release --appId=--appId=pt.rainbowapps.gardao.app.release",
    "android-prod": "ENVFILE=.env react-native run-android",
    "android-prod-release": "ENVFILE=.env react-native run-android --mode=Release --appId=--appId=pt.rainbowapps.gardao.app.release",
    "build-android-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease && cd ..",
    "build-android-dev-aab": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease && cd ..",
    "build-android-staging": "cd android && ENVFILE=.env.stg ./gradlew assembleRelease && cd ..",
    "build-android-staging-aab": "cd android && ENVFILE=.env.stg ./gradlew bundleRelease && cd ..",
    "build-android-prod": "cd android && ENVFILE=.env ./gradlew bundleRelease && cd ..",
    "build-android-prod-aab": "cd android && ENVFILE=.env ./gradlew bundleRelease && cd ..",
    "ios-dev": "react-native run-ios --scheme DEV --simulator='iPhone 15 Pro Max'",
    "ios-stg": "react-native run-ios --scheme STG --simulator='iPhone 15 Pro Max'",
    "ios-prod": "react-native run-ios --scheme PROD --simulator='iPhone 15 Pro Max'",
    "lint": "eslint .",
    "pre-commit-check": "yarn lint-staged --allow-empty",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.20",
    "@sentry/react-native": "~5.22.0",
    "@supabase/supabase-js": "^2.33.1",
    "date-fns": "^3.2.0",
    "i18next": "^23.5.1",
    "i18next-http-backend": "^2.2.2",
    "intl-pluralrules": "^2.0.1",
    "lottie-react-native": "6.7.0",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^13.2.2",
    "react-native": "0.74.2",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-config": "^1.5.1",
    "react-native-date-picker": "^4.3.5",
    "react-native-drop-shadow": "^1.0.0",
    "react-native-elements": "^3.4.3",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-in-app-review": "^4.3.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.0.2",
    "react-native-modal": "^13.0.1",
    "react-native-onesignal": "^5.2.2",
    "react-native-paper": "^5.11.4",
    "react-native-reanimated": "~3.10.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "styled-components": "^6.1.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "^0.74.84",
    "@react-native/metro-config": "^0.74.84",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "~18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.2.5",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ft-flow": "^3.0.10",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.6.3",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.0.3",
    "react-native-svg-transformer": "^1.2.0",
    "react-test-renderer": "18.2.0",
    "tsc-files": "^1.1.4",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "20.16.0"
  },
  "lint-staged": {
    "./**/*.{js,ts,jsx,tsx}": [
      "tsc-files --noEmit",
      "eslint --fix",
      "prettier --write -u"
    ]
  }
}
