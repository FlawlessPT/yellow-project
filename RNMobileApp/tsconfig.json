{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@components": ["src/components/index"],
      "@components/*": ["src/components/*"],
      "@theme": ["src/theme/index"],
      "@assets": ["src/assets/index"],
      "@hooks/*": ["src/hooks/*"]
    }
  }
}
